{
    "version": 2,
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/"
            
        } 
    ],
    "public": {
        "rewrites": [
          {
            "source": "/(.*)",
            "destination": "/build/(.*)" 
          }
        ]
      }
    
}